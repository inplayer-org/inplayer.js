{
  "name": "@inplayer-org/inplayer.js",
  "version": "2.12.3",
  "author": "InPlayer",
  "license": "MIT",
  "description": "A Javascript SDK for Inplayer's RESTful API",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/inplayer.umd.js",
  "types": "dist/index.d.ts",
  "browser": "dist/inplayer.umd.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dev": "webpack --progress --colors --watch",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "generate:docs": "yarn run yuidoc .",
    "lint": "eslint ./src --ext .js",
    "lint:js:fix": "eslint src/** --fix"
  },
  "lint-staged": {
    "*.js": [
      "prettier --tab-width 2 --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inplayer-org/inplayer.js.git"
  },
  "keywords": [
    "sdk",
    "inplayer"
  ],
  "dependencies": {
    "aws-iot-device-sdk": "^2.2.1",
    "fingerprintjs2": "^2.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.3",
    "eslint": "^6.6.0",
    "eslint-loader": "^3.0.2",
    "formdata-polyfill": "^3.0.19",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "yuidoc-lucid-theme": "^0.1.2",
    "yuidocjs": "^0.10.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
