{
  "name": "@inplayer-org/inplayer.js",
  "version": "2.12.3",
  "author": "InPlayer",
  "license": "MIT",
  "description": "A Javascript SDK for Inplayer's RESTful API",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/inplayer.min.js",
  "types": "dist/index.d.ts",
  "browser": "dist/inplayer.min.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "dev": "webpack --progress --colors --watch",
    "prebuild": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "test": "jest --watchAll",
    "generate:docs": "yarn run yuidoc .",
    "lint": "eslint ./src --ext .js",
    "lint:js:fix": "eslint src/** --fix",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --tab-width 2 --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inplayer-org/inplayer.js.git"
  },
  "keywords": [
    "sdk",
    "inplayer"
  ],
  "dependencies": {
    "@types/fingerprintjs2": "^2.0.0",
    "aws-iot-device-sdk": "^2.2.1",
    "axios": "^0.19.0",
    "fingerprintjs2": "^2.1.0",
    "lodash": "^4.17.15",
    "qs": "^6.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.2",
    "@babel/core": "^7.6.2",
    "@babel/node": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@babel/register": "^7.6.2",
    "@babel/runtime": "^7.6.2",
    "@types/aws-iot-device-sdk": "^2.2.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.141",
    "@types/qs": "^6.5.3",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "eslint": "^6.5.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.7",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^1.18.2",
    "terser-webpack-plugin": "^2.1.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0",
    "webpack-bundle-analyzer": "^3.5.2",
    "webpack-cli": "^3.3.9",
    "yuidoc-lucid-theme": "^0.1.2",
    "yuidocjs": "^0.10.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
