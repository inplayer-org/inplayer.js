{
  "name": "@inplayer/inplayer-sdk",
  "version": "0.1.9",
  "description": "A Javascript SDK for Inplayer's RESTful API",
  "main": "dist/inplayer.umd.js",
  "cjs:main": "dist/inplayer.cjs.js",
  "browser": "dist/inplayer.umd.js",
  "scripts": {
    "dev": "rollup -c --watch",
    "build": "rollup -c",
    "test": "mocha -r mock-local-storage --require babel-core/register --require isomorphic-fetch --require babel-polyfill ./test/*.test.js",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --tab-width 4 --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://repo.inplayer.com/platform/inplayer.js.git"
  },
  "babel": {
    "presets": [
      "env",
      "es2015"
    ]
  },
  "keywords": [
    "sdk",
    "inplayer"
  ],
  "author": "InPlayer",
  "license": "ISC",
  "dependencies": {
    "node-localstorage": "^1.3.0",
    "stompjs": "^2.3.3",
    "ws": "^3.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-async-to-promises": "^1.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "chai": "^4.1.2",
    "esdoc": "^1.0.4",
    "formdata-polyfill": "^3.0.9",
    "husky": "^0.14.3",
    "isomorphic-fetch": "^2.2.1",
    "lint-staged": "^6.0.0",
    "mocha": "^4.0.1",
    "mocha-jsdom": "^1.1.0",
    "mock-local-storage": "^1.0.5",
    "prettier": "^1.9.2",
    "rollup": "^0.52.0",
    "rollup-plugin-async": "^1.2.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-buble": "^0.18.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.1.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "uglify-es": "^3.2.0",
    "yuidoc-lucid-theme": "^0.1.2"
  }
}
