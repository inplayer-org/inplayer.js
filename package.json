{
	"name": "@inplayer-org/inplayer.js",
	"version": "2.0.0",
	"author": "InPlayer",
	"license": "MIT",
	"description": "A Javascript SDK for Inplayer's RESTful API",
	"publishConfig": {
		"access": "public"
	},
	"main": "dist/inplayer.umd.min.js",
	"cjs:main": "dist/inplayer.cjs.js",
	"browser": "dist/inplayer.umd.min.js",
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"clean": "rimraf dist",
		"build": "npm run clean && cross-env NODE_ENV=production webpack --config ./webpack.config.js",
		"dev": "webpack --progress --colors --watch",
		"test": "mocha -r babel-register -r mock-local-storage -r jsdom-global/register -r isomorphic-fetch -r @babel/polyfill ./test/*.test.js",
		"precommit": "lint-staged"
	},
	"lint-staged": {
		"*.js": [ "prettier --tab-width 4 --trailing-comma es5 --single-quote --write", "git add" ]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/inplayer-org/inplayer.js.git"
	},
	"keywords": [ "sdk", "inplayer" ],
	"dependencies": {
		"aws-iot-device-sdk": "^2.2.1",
		"es6-promise": "^4.2.2",
		"isomorphic-fetch": "^2.2.1"
	},
	"devDependencies": {
		"lodash": "^4.17.10",
		"@babel/polyfill": "^7.0.0",
		"babel-cli": "^6.24.1",
		"babel-core": "^6.26.3",
		"babel-eslint": "^8.2.6",
		"babel-loader": "^7.1.5",
		"babel-minify-webpack-plugin": "^0.3.1",
		"babel-plugin-inline-replace-variables": "^1.3.1",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-rewire": "^1.0.0",
		"babel-plugin-transform-object-rest-spread": "^6.23.0",
		"babel-preset-env": "^1.7.0",
		"babel-register": "^7.0.0",
		"chai": "^4.1.2",
		"cross-env": "^5.2.0",
		"esdoc": "^1.1.0",
		"eslint": "^5.0.1",
		"eslint-loader": "^2.0.0",
		"formdata-polyfill": "^3.0.11",
		"husky": "^0.14.3",
		"jsdom": "11.6.2",
		"jsdom-global": "3.0.2",
		"json-loader": "^0.5.7",
		"lint-staged": "^6.0.0",
		"lodash-webpack-plugin": "^0.11.5",
		"mocha": "^4.0.1",
		"mocha-jsdom": "^1.2.0",
		"mock-local-storage": "^1.0.5",
		"prettier": "^1.14.2",
		"uglify-js": "^3.4.8",
		"webpack": "^4.17.1",
		"webpack-bundle-analyzer": "^2.13.1",
		"webpack-cli": "^2.1.5",
		"yuidoc-lucid-theme": "^0.1.2"
	},
	"files": [ "dist" ]
}
