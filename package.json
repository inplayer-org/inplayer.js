{
  "name": "@inplayer-org/inplayer.js",
  "version": "3.4.1",
  "author": "InPlayer",
  "license": "MIT",
  "description": "A Javascript SDK for Inplayer's RESTful API",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/inplayer.min.js",
  "types": "dist/index.d.ts",
  "browser": "dist/inplayer.min.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "webpack --progress --colors --watch",
    "prebuild": "rimraf dist",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:js:fix": "eslint src/** --fix",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "prepublishOnly": "yarn build:prod",
    "build:typedoc": "typedoc"
  },
  "lint-staged": {
    "*.js": [
      "prettier --tab-width 2 --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inplayer-org/inplayer.js.git"
  },
  "keywords": [
    "sdk",
    "inplayer"
  ],
  "dependencies": {
    "aws-iot-device-sdk": "^2.2.6",
    "axios": "^0.19.2",
    "core-js": "^3.6.5",
    "fingerprintjs2": "^2.1.2",
    "lodash": "^4.17.20",
    "qs": "^6.9.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.1",
    "@types/aws-iot-device-sdk": "^2.2.2",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/lodash": "^4.14.164",
    "@types/qs": "^6.9.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^6.2.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^2.1.2",
    "terser-webpack-plugin": "^4.2.3",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5",
    "webpack": "^4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^3.3.12"
  }
}
