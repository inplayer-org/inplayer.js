{
  "name": "@inplayer-org/inplayer.js",
  "version": "3.0.0-beta.7",
  "author": "InPlayer",
  "license": "MIT",
  "description": "A Javascript SDK for Inplayer's RESTful API",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/inplayer.min.js",
  "types": "dist/index.d.ts",
  "browser": "dist/inplayer.min.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "dev": "webpack --progress --colors --watch",
    "prebuild": "rimraf dist",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.config.js",
    "test": "jest --watchAll",
    "lint": "eslint 'src/**/*.{js,ts,tsx}' --quiet --fix",
    "lint:js:fix": "eslint src/** --fix",
    "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
    "prepublishOnly": "yarn build:prod",
    "generate:docs": "typedoc",
    "postgenerate:docs": "surge ./docs --domain inplayer-js.v3.surge.sh"
  },
  "lint-staged": {
    "*.js": [
      "prettier --tab-width 2 --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/inplayer-org/inplayer.js.git"
  },
  "keywords": [
    "sdk",
    "inplayer"
  ],
  "dependencies": {
    "aws-iot-device-sdk": "^2.2.1",
    "axios": "^0.19.0",
    "fingerprintjs2": "^2.1.0",
    "lodash": "^4.17.15",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@babel/register": "^7.7.7",
    "@babel/runtime": "^7.7.7",
    "@types/aws-iot-device-sdk": "^2.2.0",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "^25.2.1",
    "@types/lodash": "^4.14.149",
    "@types/qs": "^6.9.0",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "lodash-webpack-plugin": "^0.11.5",
    "prettier": "^1.19.1",
    "surge": "^0.21.3",
    "terser-webpack-plugin": "^3.0.1",
    "ts-jest": "^24.2.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.7.3",
    "webpack": "^4.41.4",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
